deluge.i.dfl.mn {
	encode zstd gzip
	reverse_proxy deluge:8112
	tls security@duffeman.co.uk {
		client_auth {
			mode require_and_verify
			trusted_ca_cert_file /config/root.public.pem
		}
		dns cloudflare {env.CLOUDFLARE_API_TOKEN}
	}
}

